   "source": [
    "file = open(\"iris.data\")\n",
    "data = file.readlines()\n",
    "data = [d.split(',') for d in data[:-1]]\n",
    "\n",
    "X = [d[:-1] for d in data]\n",
    "for i in range(len(X)):\n",
    "    X[i] = [float(x) for x in X[i]]\n",
    "\n",
    "\n",
    "Y = [d[-1] for d in data]\n",
    "dic = {}\n",
    "for k,v in enumerate(set(Y)):\n",
    "    dic[v]=k\n",
    "Y = [dic[v] for v in Y]"
   ]
